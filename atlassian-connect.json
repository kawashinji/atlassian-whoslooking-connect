{
    "key": "${addonKey}",
    "name": "Who's Looking for Jira Cloud",
    "description": "Who&apos;s Looking for Jira Cloud. See who else is looking at a Jira issue.",
    "vendor": {
        "name": "Atlassian",
        "url": "http://www.atlassian.com"
    },
    "baseUrl": "https://sboyapati2-whoslooking.public.atlastunnel.com",
    "authentication": {
        "type": "jwt"
    },
    "enableLicensing": false,
    "lifecycle": {
        "installed": "/installed",
        "uninstalled": "uninstalled"
    },
    "modules": {
        "webPanels": [{
            "key" : "whos-looking",
            "name": {
                "value": "Who's Looking?"
            },
            "url": "/poller?issue_key={issue.key}",
            "location": "atl.jira.view.issue.right.context"
        }]
    },
    "apiMigrations": {
        "gdpr": true,
        "context-qsh": true,
        "signed-install": true
    },

    "scopes": ["READ"]
}

